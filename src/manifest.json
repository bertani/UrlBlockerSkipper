{
  "name": "UrlBlockerSkipper",
  "version": "0.1",
  "description": "Skips url blocker",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "http://*/*"
  ]
}
